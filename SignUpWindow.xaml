<Window x:Class="MyShopProject.SignUpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyShopProject"
        mc:Ignorable="d"
        Title="SignUpWindow" Height="450" Width="800">
    <Canvas>
        <Button Name="signupButton"  Width="80" Height="35" Content="Sign up" Click="signupButton_Click" Canvas.Left="405" Canvas.Top="249" HorizontalAlignment="Left" VerticalAlignment="Top"></Button>
        <TextBlock Text="Username:" Canvas.Left="298" Canvas.Top="152"/>
        <TextBox Name="usernameTextBox" Width="150" Height="35" Canvas.Left="370" Canvas.Top="142"/>
        <TextBlock Text="Password:" Canvas.Left="302" Canvas.Top="192"/>
        <PasswordBox PasswordChar="*" x:Name="passwordBox" Width="150" Height="35" Canvas.Left="370" Canvas.Top="182"/>

    </Canvas>
</Window>
